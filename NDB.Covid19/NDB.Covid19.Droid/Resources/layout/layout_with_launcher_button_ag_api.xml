<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
	<ImageView 
		android:src="@drawable/ic_start_logo_ag_api"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:id="@+id/ic_start_logo"
		android:layout_centerInParent="true"
		android:layout_margin="10dp"
	/>
	<Button
		android:text="Start"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="10dp"
		android:layout_marginEnd="10dp"
		android:layout_marginTop="20dp"
		android:layout_marginBottom="34dp"
		android:layout_centerHorizontal="true"
		style="@style/DefaultButton"
		android:id="@+id/launcher_button"
		android:layout_above="@id/continue_in_en_layout"
	/>
	<RelativeLayout
	    android:id="@+id/continue_in_en_layout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
		android:layout_centerHorizontal="true"
		android:layout_above="@id/launcer_icon_imageview"
        android:paddingStart="16dp"
        android:paddingEnd="14.65dp"
        android:gravity="center">
        <TextView
            android:id="@+id/continue_in_en_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            style="@style/MessageListItemDateText"
        />
        <View
            android:layout_width="10.7dp"
            android:layout_height="18.15dp"
			android:layout_marginLeft="8dp"
			android:layout_toRightOf="@+id/continue_in_en_text"
			android:background="@drawable/ic_back_icon"
		/>
    </RelativeLayout>
	<ImageView 
		android:src="@drawable/health_department_logo"
		android:layout_width="200dp"
		android:layout_height="46dp"
		android:id="@+id/launcer_icon_imageview"
		android:layout_centerInParent="true"
		android:layout_alignParentBottom="true"
		android:layout_margin="24dp"
	/>
</RelativeLayout>